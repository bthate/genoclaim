#!/bin/bash
bin/clean
python3 setup.py sdist
#python3 -mvirtualenv -p python3 --no-site-packages envs/latest
python3 -mvirtualenv -p python3 envs/latest
source envs/latest/bin/activate
envs/latest/bin/python3 envs/latest/bin/pip3 install -U -f dist -r requirements.txt
envs/latest/bin/python3 envs/latest/bin/sphinx-build -a -E docs _build/html
envs/latest/bin/python3 envs/latest/bin/sphinx-build -a -E docs _build/localmedia
